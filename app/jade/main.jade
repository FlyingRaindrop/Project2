doctype html
html(lang="ru-RU")
	head
		include _common/head.jade
		link(rel="stylesheet", href="css/_layout/style.css") 
	body
		//[if IE 9]
		<div class="ie9">
		//[endif]
		.wrapper
			.page
				include _common/_mixins.jade
				include _common/header.jade
				include _common/mainNav.jade
				.content.clearfix
					.container
						include _common/breadcrumbs.jade
						include _common/sidebar.jade
						.right-content
							include _common/select.jade
							block content
							include _common/pages.jade
						include _common/mainInfo.jade
			include _common/footer.jade
		script(src="bower/jquery/jquery.js")
		script(src="js/jquery-ui.min.js")
		script(src="js/placeholder.js", type="text/javascript")
		script(src="js/placeholder-init.js", type="text/javascript")
		script(src="js/main.js", type="text/javascript")





